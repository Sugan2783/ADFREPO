{
	"name": "dataflow_PRODUCT_DIM_SCD2",
	"properties": {
		"folder": {
			"name": "ADF_TRAINING_DATAFLOW"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ADLS_PARAM",
						"type": "DatasetReference"
					},
					"name": "productjoin"
				},
				{
					"dataset": {
						"referenceName": "PRODUCT_DIM_SCD2",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "lookup1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PRODUCT_ID as short,",
				"          PRODUCT_NAME as string,",
				"          PRODUCT_PRICE as double,",
				"          PRODUCT_DESCRIPTION as string,",
				"          IS_ACTIVE as string,",
				"          PRODUCT_SUBCATEGORY_ID as short,",
				"          PRODUCT_SUBCATEGORY_NAME as string,",
				"          PRODUCT_SIZE as string,",
				"          PRODUCT_CATEGORY_NAME as string,",
				"          PRODUCT_CATEGORY_ID as short,",
				"          CREATE_USER_DATE as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> productjoin",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"productjoin, source1 lookup(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1 select(mapColumn(",
				"          PRODUCT_ID = productjoin@PRODUCT_ID,",
				"          PRODUCT_NAME = productjoin@PRODUCT_NAME,",
				"          PRODUCT_PRICE = productjoin@PRODUCT_PRICE,",
				"          PRODUCT_DESCRIPTION = productjoin@PRODUCT_DESCRIPTION,",
				"          IS_ACTIVE = productjoin@IS_ACTIVE,",
				"          PRODUCT_SUBCATEGORY_ID = productjoin@PRODUCT_SUBCATEGORY_ID,",
				"          PRODUCT_SUBCATEGORY_NAME = productjoin@PRODUCT_SUBCATEGORY_NAME,",
				"          PRODUCT_SIZE = productjoin@PRODUCT_SIZE,",
				"          PRODUCT_CATEGORY_NAME = productjoin@PRODUCT_CATEGORY_NAME,",
				"          PRODUCT_CATEGORY_ID = productjoin@PRODUCT_CATEGORY_ID,",
				"          CREATE_USER_DATE = productjoin@CREATE_USER_DATE,",
				"          lkp_PRODUCT_DIM_KEY = PRODUCT_DIM_KEY,",
				"          lkp_PRODUCT_ID = lkpproductdim@PRODUCT_ID,",
				"          lkp_PRODUCT_NAME = lkpproductdim@PRODUCT_NAME,",
				"          lkp_PRODUCT_PRICE = lkpproductdim@PRODUCT_PRICE,",
				"          lkp_PRODUCT_DESCRIPTION = lkpproductdim@PRODUCT_DESCRIPTION,",
				"          lkp_IS_ACTIVE = lkpproductdim@IS_ACTIVE,",
				"          lkp_PRODUCT_SUBCATEGORY_ID = lkpproductdim@PRODUCT_SUBCATEGORY_ID,",
				"          lkp_PRODUCT_SUBCATEGORY_NAME = lkpproductdim@PRODUCT_SUBCATEGORY_NAME,",
				"          lkp_PRODUCT_SIZE = lkpproductdim@PRODUCT_SIZE,",
				"          lkp_PRODUCT_CATEGORY_NAME = lkpproductdim@PRODUCT_CATEGORY_NAME,",
				"          lkp_PRODUCT_CATEGORY_ID = lkpproductdim@PRODUCT_CATEGORY_ID,",
				"          lkp_CREATE_USER_DATE = lkpproductdim@CREATE_USER_DATE",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}